[{"_id":"T8YCEhafZ9puwmeS4","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Design","domainTermNew":"Design","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"The complete functional specification for an Application or set of Applications","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Design","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"DXBwvg9Ns8WYYgzor","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Design Version","domainTermNew":"Design Version","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"An iteration of a Design.  The first Design Version is created freely.  Further Design Versions are created by adding Design Updates to the current Design Version","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Design Version","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"doK4oWFQc5YNJKTTr","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Design Update","domainTermNew":"Design Update","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"A specification of changes to be made to a Design Version","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Design Update","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"EHqw2Q6mDNQC2SvEJ","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Designer","domainTermNew":"Designer","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"A person who creates and maintains the functional specification of an Application.","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Designer","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"i7kGLjnso4tKbF57k","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Developer","domainTermNew":"Developer","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"A person who turns a Design into a working, deliverable and testable piece of software.","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Developer","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"7Zk6mJF42JhW5hoR4","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Manager","domainTermNew":"Manager","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"A person who organises the work of Developers.","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Manager","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"kEMPrFrHPgRm44fEt","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Application","domainTermNew":"Application","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"A standalone application that is part of an overall Design.  A Design could have one or more Applications.","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Application","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"M9A8NP6bMRQ5FjPT3","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Design Section","domainTermNew":"Design Section","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"A subdivision of a Design with no functional significance.  Used only to organise the Design specification into sections.  A Design section may have any number of sub-sections if these are required.","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Design Section","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"inN6XPKYegn5pAm8K","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Feature","domainTermNew":"Feature","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"A basic use case or user story in the Design specification.  A Feature should describe a discreet bit of functionality offered by the Application.  It is not a testable item in itself.  A Feature will have one or more testable Scenarios within it that describe the behaviour of the Feature.","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Feature","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"gaMnd2pkLrHaEXLwZ","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Feature Aspect","domainTermNew":"Feature Aspect","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"A non functional division inside a Feature that allows Scenarios to be grouped into different types to aid understanding of the Feature","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Feature Aspect","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"FehEGF8GAgiNGdNTy","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Scenario","domainTermNew":"Scenario","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"A single testable case within a Feature.  A Scenario is the atomic functional unit of the Design and should be able to be described and tested by example using Given / When / Then syntax.","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Scenario","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"mdcsA9zuYS4MGZFPo","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Scenario Step","domainTermNew":"Scenario Step","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"An element of the specification of a Scenario by example.  Each step represents one Given / Then / When / And statement.  Scenarios written with steps in this format can be converted to Cucumber tests.","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Scenario Step","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"eMiRyLJS6NX7qLBGx","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Domain Dictionary","domainTermNew":"Domain Dictionary","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"A list of terms which have a specific meaning for a Design and their definitions.  A domain term should have one meaning within the Design scope and always be used in the Design and the developed code.  Domain terms are highlighted throughout the Design if they are entered in the same format as in the dictionary (usually with an initial capital).  Plurals of domain terms are also domain terms.","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Domain Dictionary","markInDesign":true,"isNew":false,"isChanged":false},{"_id":"RAiy2vmsxmaXD4bvs","designId":"qdSv6RRPZrPDjfmqQ","designVersionId":"fK5vpySPHgWbEnZnt","domainTermOld":"Work Package","domainTermNew":"Work Package","domainTextRaw":{"entityMap":{},"blocks":[{"key":"5efv7","text":"A subset of a Design Version or Design Update given to a Developer by a Manager to implement.  A Scenario in a particular Design Version or Design Update may not appear in more than one concurrent Work Package.  A Developer may adopt one or more Work Packages to give them a view of the work they are implementing.","type":"unstyled","depth":0,"inlineStyleRanges":[],"entityRanges":[],"data":{}}]},"sortingName":"Work Package","markInDesign":true,"isNew":false,"isChanged":false}]